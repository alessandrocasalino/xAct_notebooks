Cell["\<\
We define some functions that simplify the computation of the results. \
Instead of calling ToxPand we will call MyToxPand. Also, org will collect \
perturbation order in the results.\
\>", "Text",
 CellChangeTimes->{{3.794045707655466*^9, 3.794045751749323*^9}, {
  3.794045821674775*^9, 
  3.794045867035226*^9}},ExpressionUUID->"4729aecf-7b6c-4e62-ab9b-\
75f94b6f418a"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"MakeBoxes", "[", 
   RowBox[{"\[Epsilon]", ",", "StandardForm"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"StyleBox", "[", 
   RowBox[{
    RowBox[{"ToString", "[", "$PerturbationParameter", "]"}], ",", 
    RowBox[{"FontColor", "\[Rule]", 
     RowBox[{"RGBColor", "[", 
      RowBox[{"0.0", ",", " ", "0.0", ",", " ", "1"}], "]"}]}]}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"MyToxPand", "[", 
   RowBox[{"expr_", ",", "gauge_", ",", "order_"}], "]"}], ":=", 
  RowBox[{"ToxPand", "[", 
   RowBox[{
   "expr", ",", "dg", ",", "u", ",", "du", ",", "h", ",", " ", "gauge", ",", 
    "order"}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"org", "[", "expr_", "]"}], ":=", 
  RowBox[{"NoScalar", "@", 
   RowBox[{"Collect", "[", 
    RowBox[{
     RowBox[{"ContractMetric", "[", "expr", "]"}], ",", 
     "$PerturbationParameter", ",", "ToCanonical"}], "]"}]}]}]}], "Input",
 CellLabel->"In[10]:=",ExpressionUUID->"27c84417-2e7f-49fb-a87f-0f465ed90e80"]
}, Open  ]],

